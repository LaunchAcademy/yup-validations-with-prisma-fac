{
  "scripts": {
    "start": "node src/app.js",
    "clean": "rm -rf ./public/dist",
    "dev": "yarn run clean && ./node_modules/.bin/nodemon src/app.js",
    "dev:debug": "./node_modules/.bin/nodemon --inspect-brk src/app.js",
    "console": "node --experimental-repl-await ./src/console.js",
    "db:seed": "node ./src/prisma/seed.js",
    "db:test:migrate": "NODE_ENV='test' yarn run migrate:latest",
    "db:e2e:migrate": "NODE_ENV='e2e' yarn run migrate:latest",
    "generate-secret": "./scripts/generate-secret.js",
    "dev:e2e": "NODE_ENV=e2e PORT=8765 nodemon src/app.js"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "node src/prisma/seed.js"
  },
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "name": "unicorns-server",
  "engines": {
    "node": "^16.18"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "babel-jest": "^29.6.1",
    "errorhandler": "^1.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "install-peerdeps": "^3.0.3",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "webpack": "^5.3.2",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-hot-middleware": "^2.25.4"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-session": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-handlebars": "^5.2.0",
    "morgan": "^1.10.0",
    "npx": "^10.2.2",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "prisma": "^5.0.0",
    "yup": "^1.2.0"
  }
}
